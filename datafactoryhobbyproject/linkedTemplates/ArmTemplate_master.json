{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactoryhobbyproject"
		},
		"DataLake_linked_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DataLake_linked'"
		},
		"DataLake_linked_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mystorageaccountrachit.dfs.core.windows.net/"
		},
		"HttpServer_Github_Linked_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().BaseURL}"
		},
		"FileCreationTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/64885e05-1c68-4073-9065-a3dedd19eb57/resourceGroups/my-resource-group/providers/Microsoft.Storage/storageAccounts/mystorageaccountrachit"
		},
		"newTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/64885e05-1c68-4073-9065-a3dedd19eb57/resourceGroups/my-resource-group/providers/Microsoft.Storage/storageAccounts/mystorageaccountrachit"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactoryhobbyproject_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DataLake_linked_accountKey": {
						"value": "[parameters('DataLake_linked_accountKey')]"
					},
					"DataLake_linked_properties_typeProperties_url": {
						"value": "[parameters('DataLake_linked_properties_typeProperties_url')]"
					},
					"HttpServer_Github_Linked_properties_typeProperties_url": {
						"value": "[parameters('HttpServer_Github_Linked_properties_typeProperties_url')]"
					},
					"FileCreationTrigger_properties_typeProperties_scope": {
						"value": "[parameters('FileCreationTrigger_properties_typeProperties_scope')]"
					},
					"newTrigger_properties_typeProperties_scope": {
						"value": "[parameters('newTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}